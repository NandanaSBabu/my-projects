import numpy as np
import matplotlib.pyplot as plt
data=np.random.randn(100)
plt.figure(figsize=(8,4))
plt.subplot(2,3,1)
plt.plot(data,color="green")
plt.xlabel('value')
plt.ylabel('distribution')
plt.grid(True)
plt.subplot(2,3,2)
plt.stem(data,basefmt="purple",linefmt="purple",markerfmt="purple")
plt.xlabel('value')
plt.ylabel('distribution')
plt.grid(True)
plt.subplot(2,3,3)
plt.bar(range(len(data)),data)
plt.xlabel('value')
plt.ylabel('distribution')
plt.grid(True)
plt.subplot(2,3,4)
plt.scatter(range(len(data)),data,color="red")
plt.xlabel('value')
plt.ylabel('distribution')
plt.grid(True)
plt.subplot(2,3,5)
plt.boxplot(data)
plt.xlabel('value')
plt.ylabel('distribution')
plt.grid(True)
plt.tight_layout()
plt.show()
